<block>
  <name>Decrypt Secret</name>
  <key>nacl_decrypt_secret</key>
  <category>NACL</category>
  <import>import nacl</import>
  <make>nacl.decrypt_secret($filename_key)</make>
  <param>
    <name>Filename key</name>
    <key>filename_key</key>
    <type>string</type>
  </param>
  <sink>
    <name>Msg encrypted</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg decrypted</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
